{
  "extends": ["airbnb-base", "prettier"],
  "env": {
    "jest/globals": true
  },
  "plugins": ["prettier", "jest"],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "func-names": ["error", "as-needed"],
    "prettier/prettier": "error",
    "no-unused-expressions": "off"
  }
}
